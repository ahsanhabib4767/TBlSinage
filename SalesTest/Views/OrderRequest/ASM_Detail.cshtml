@model SalesTest.Models.RequestMaster
@{
    Layout = null;
}
<table class="table table-striped projects">
    <thead>
        <tr>
            <th class="column-title">Uploaded Image</th>
            <th class="column-title">Outlet Code</th>
            <th class="column-title">Outlet Name</th>

 
        </tr>
    </thead>
  
        <tbody>
            <tr>
                <td>
                    <img src="~/AppFile/Images/@Html.DisplayFor(model =>model.PicUrl)" height="300" width="300" />
                </td>
                <td>
                    @Html.DisplayFor(model => model.OutletCode)
                </td>
                <td>
                    @Html.DisplayFor(model => model.OutLetName)
                </td>
               



            </tr>

        </tbody>
    
</table>
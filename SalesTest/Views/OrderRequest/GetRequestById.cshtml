@model IEnumerable<SalesTest.Models.RequestMaster>
    @{
        Layout = null;
    }

    <ul class="nav navbar-right panel_toolbox">
        <li>
            <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
        </li>
        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <a class="dropdown-item" href="#">Settings 1</a>
                <a class="dropdown-item" href="#">Settings 2</a>
            </div>
        </li>
        <li>
            <a class="close-link"><i class="fa fa-close"></i></a>
        </li>
    </ul>
    <div class="clearfix"></div>

    <div class="col-md-12 col-sm-12  ">
        <div class="x_panel">
            <div class="x_content">
                <div class="table-responsive">
         <table class="table table-striped jambo_table bulk_action">
                        <thead>
                            <tr class="headings">
                                <th>
                                    <input type="checkbox" id="check-all" class="flat">
                                </th>
                                <th class="column-title">Uploaded Image</th>
                                <th class="column-title">Outlet Code</th>
                                <th class="column-title">Region Name</th>
                                <th class="column-title">Area Name</th>
                                <th class="column-title">Ce Area Name</th>
                                <th class="column-title">OwnerName</th>
                                <th class="column-title no-link last">
                                    <span class="nobr">Action</span>
                                </th>
                                <th class="bulk-actions" colspan="7">
                                    <a class="antoo" style="color:#fff; font-weight:500;">Bulk Actions ( <span class="action-cnt"> </span> ) <i class="fa fa-chevron-down"></i></a>
                                </th>
                            </tr>
                        </thead>
                      @foreach (var item in Model)
                      {

                        <tbody>
                            <tr class="even pointer">
                                <td class="a-center ">
                                    @Html.HiddenFor(model => item.id)
                                </td>
                                <td>
                                    <img src="~/AppFile/Images/@item.PicUrl" height="40" width="40" />
                                </td>
                                <td class="">@Html.DisplayFor(model => item.OutletCode)</td>
                                <td class="">@Html.DisplayFor(model => item.RegionName)</td>
                                <td class="">@Html.DisplayFor(model => item.AreaName)</td>
                                <td class="">@Html.DisplayFor(model => item.CeAreaName)</td>
                                <td class="">@Html.DisplayFor(model => item.OwnerName)</td>
                                <td class="">
                                    <select id="AppFlag1" name="AppFlag1" class="form-control">
                                    <option value="A" selected>Accepted</option>
                                    <option value="R" selected>Rejected</option>
                                    </select>
                                </td>
                                <td class="last">
                                    @Html.ActionLink("EditRequest", "EditRequest", new { id = item.id})
                                    
                                </td>
                                
                            </tr>
                        </tbody>

                      }
                    </table>

                </div>


            </div>
        </div>
    </div>





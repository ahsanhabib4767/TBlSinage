@model IEnumerable<SalesTest.Models.VendorConfirm>
@{
    Layout = "~/Views/Shared/_LayoutTwo.cshtml";
}
@{
    ViewBag.Title = "Work Request Status";
}
<!-- page content -->
<div class="right_col" role="main">
    <div class="">
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 ">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Satus of Request<small>By SE</small></h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <li>
                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="#">Settings 1</a>
                                    <a class="dropdown-item" href="#">Settings 2</a>
                                </div>
                            </li>
                            <li>
                                <a class="close-link"><i class="fa fa-close"></i></a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="card-box table-responsive">
                                   
                                    <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                                        <thead>
                                            <tr>
                                                <th>Before Signage</th>
                                                <th>After Signage</th>
                                                <th>Outlet Code</th>
                                                <th>Region Name</th>
                                                <th>Ce Area Name</th>
                                                <th>Received</th>
                                                <th>Cluster</th>
                                                <th>Channel</th>
                                                <th>Route</th>
                                                <th>Route2</th>
                                            </tr>
                                        </thead>
                                        @foreach (var item in Model)
                                        {
                                            <tbody>
                                                <tr>
                                                    <td>
                                                        <img src="~/AppFile/Images/@item.PicUrl" height="50" width="50" />
                                                    </td>
                                                    <td>
                                                        <img src="~/AppFile/Images/@item.PicUrl2" height="50" width="50" />
                                                    </td>
                                                    <td>
                                                        @Html.TextBoxFor(model => item.OutletCode)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(model => item.RegionName)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(model => item.CeAreaName)
                                                    </td>
                                                    <td style="color:red">
                                                        @if (item.SEfeedback == "Done")
                                                        {
                                                            <h4>Done</h4>
                                                        }
                                                        @if (item.SEfeedback == "Notdone")
                                                        {
                                                            <h3>Not Done</h3>
                                                        }
                                                        @if (item.SEfeedback == "")
                                                        {
                                                            <h3>Not Finalized</h3>
                                                        }

                                                    </td>
                                                    <td>@Html.DisplayFor(model => item.cluster)</td>
                                                    <td>@Html.DisplayFor(model => item.ChannelName)</td>
                                                    <td>@Html.DisplayFor(model => item.SubRoute)</td>
                                                    <td>Route2</td>
                                                </tr>

                                            </tbody>
                                        }
                                    </table>


                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /page content -->

<div id="tabledata"></div>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script>
    $('#showReport').click(function () {
        $("#ajax_load").show();
        var wValue = $("#ReqID").val();


        $.ajax({
            url: "@Url.Action("Workorder", "Report")",
            dataType: "HTML",
            type: "POST",
            data: { reqid: wValue },
            cache: false,
            async: true,
            success: function (data) {
                $("#tabledata").html(data);
            }
        });



    })

</script>

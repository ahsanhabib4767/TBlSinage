@{
    Layout = "~/Views/Shared/_LayoutTwo.cshtml";
}
@model SalesTest.ModelData.VendorConfirmM
<div class="right_col" role="main">
    <div class="">
        <div class="page-title">

        </div>
        <div class="x_panel">
            <a class="btn btn-app">
                <span class="badge bg-red" id="itemss"></span>
                <i class="fa fa-bullhorn"></i> Notifications
            </a>
            <div class="x_content">
                <div class="x_title">
                    এইখান থেকে নির্বাচন করুন
                    @Html.DropDownList("VenReq", null, "Select Request", new { @class = "form-control" })

                </div>
            </div>

        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Data</h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <li>
                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="#">Settings 1</a>
                                    <a class="dropdown-item" href="#">Settings 2</a>
                                </div>
                            </li>
                            <li>
                                <a class="close-link"><i class="fa fa-close"></i></a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    @using (Html.BeginForm("SaveData", "Vendor", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                        <div class="x_content">
                            <div class="col-md-4">
                                @Html.HiddenFor(model => model.id)
                                @*@Html.TextBox("OrderBy", null, new { @class = "form-control", @readonly = "readonly" })
                                    @Html.TextBox("VenReqID", null, new { @class = "form-control", @readonly = "readonly" })*@
                                @Html.HiddenFor(model => model.OrderBy)
                                @Html.HiddenFor(model => model.VenReqID)
                                <label>RequestID:</label>
                                @Html.TextBox("ReqID", null, new { @class = "form-control", @readonly = "readonly" })

                                <label>Region Name:</label>
                                @Html.TextBox("RegionName", null, new { @class = "form-control", @readonly = "readonly" })

                                @Html.HiddenFor(model => model.AreaName)
                                @Html.HiddenFor(model => model.DbName)

                                <label>Ce Area</label>
                                <input type="hidden" id="VehicleDes" />
                                @Html.TextBox("CeAreaName", null, new { @class = "form-control", @readonly = "readonly" })

                                @Html.HiddenFor(model => model.cluster)
                                <label>PSR Name</label>
                                @Html.TextBox("PSRName", null, new { @class = "form-control", @readonly = "readonly" })
                                <label style="color:blue">মেসেজ*</label>
                                @Html.TextBox("feedback", null, new { @class = "form-control", @readonly = "readonly" })


                            </div>
                            <div class="col-md-4">
                                @Html.HiddenFor(model => model.PicUrl)
                                @Html.HiddenFor(model => model.VenID)
                                @Html.HiddenFor(model => model.sinagetype)
                                @Html.HiddenFor(model => model.otherbrand)
                                @Html.HiddenFor(model => model.SE)
                                @Html.HiddenFor(model => model.dateLimit)
                                @Html.HiddenFor(model => model.dateLimitfinal)
                                @Html.HiddenFor(model => model.SubRoute)
                                @Html.HiddenFor(model => model.OutletCode)
                                @Html.HiddenFor(model => model.ContactNumTwo)
                                @Html.HiddenFor(model => model.SE)
                                @Html.HiddenFor(model => model.ChannelName)
                                <label>Outlet Name</label>
                                @Html.TextBox("OutLetName", null, new { @class = "form-control", @readonly = "readonly" })
                                <label>Outlet Address</label>
                                @Html.TextBox("Address", null, new { @class = "form-control", @readonly = "readonly" })

                                <label>Owner Name</label>
                                @Html.TextBox("OwnerName", null, new { @class = "form-control", @readonly = "readonly" })
                                <label>Contact Number</label>
                                @Html.TextBox("ContactNum", null, new { @class = "form-control", @readonly = "readonly" })


                                <label style="color:blue">ফাইনাল ডেইট লিমিট,দিন বাকি থাকার আগেই কাজ সম্পাদন করুন**</label>
                                <h4><label style="color:red">আপনার @Html.TextBox("diff", null, "Select One", new { @class = "form-control", @readonly = "readonly" }) দিন বাকি আছে</label></h4>
                            </div>
                            <div class="col-md-4">

                                @Html.HiddenFor(model => model.signageheight)
                                @Html.HiddenFor(model => model.signagewidth)
                                @Html.HiddenFor(model => model.remarks)
                                @Html.HiddenFor(model => model.msg)
                                @Html.HiddenFor(model => model.ChannelName)


                                <label>Signage Type</label>
                                @Html.TextBox("sinage", null, "Select One", new { @class = "form-control", @readonly = "readonly" })
                                <label>Other Brand</label>
                                @Html.TextBox("otherbrandd", null, "Select One", new { @class = "form-control", @readonly = "readonly" })


                                <label style="color:blue">New Signage Height(উচ্চতা)</label>
                                @Html.TextBox("signageheightNew", null, new { @class = "form-control", @readonly = "readonly" })
                                <label style="color:blue">New Signage Width(প্রস্থ)</label>
                                @Html.TextBox("signagewidthNew", null, new { @class = "form-control", @readonly = "readonly" })
                                @Html.HiddenFor(model => model.AppFlag1)
                                @Html.HiddenFor(model => model.AppFlag3)
                            </div>
                            <br />
                            <br />

                            <div class="col-md-6 offset-md-4">
                                <label>After Signage Picture</label>
                                <input type="file" id="UploadImageb" name="UploadImageb" class="form-control" required="required" />
                                <br />
                                <button type="submit" id="btnClick" class="btn btn-primary btn-sm btn-block">
                                    Save Request
                                </button>
                                <input type="button" id="btnReset" value="Reset" class="btn btn-primary" onclick="ReLoad()" ; />
                            </div>
                        </div>


                    }
                </div>
            </div>
        </div>
    </div>
</div>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script type="text/javascript">
    var items = $("#VenReq option").length - 1;
    document.getElementById("itemss").innerHTML = items;
        $('#VenReq').change(function(){
         $.ajax({
                url: "@Url.Action("GetVendorLastRequest", "Vendor")",
                data: { reqid: $("#VenReq").val() },
            type: "GET",
            dataType: "json",
            success: function (data) {
                $("#id").val(data[0].id);
                $("#ReqID").val(data[0].ReqID);
                $("#VenReqID").val(data[0].VenReqID);
                $("#VenID").val(data[0].VenID);
                $("#msg").val(data[0].msg);
                $("#dateLimit").val(new Date(parseInt(data[0].dateLimit.substr(6))).format("m-d-y"));
                $("#dateLimitfinal").val(new Date(parseInt(data[0].dateLimitfinal.substr(6))).format("m-d-y"));
                $("#RegionName").val(data[0].RegionName);
                $("#AreaName").val(data[0].AreaName);
                $("#CeAreaName").val(data[0].CeAreaName);
                $("#DbName").val(data[0].DbName);
                $("#cluster").val(data[0].cluster);
                $("#PSRName").val(data[0].PSRName);
                $("#SubRoute").val(data[0].SubRoute);
                $("#OutletCode").val(data[0].OutletCode);
                $("#OutLetName").val(data[0].OutLetName);
                $("#Address").val(data[0].Address);
                $("#OwnerName").val(data[0].OwnerName);
                $("#ContactNum").val(data[0].ContactNum);
                $("#ChannelName").val(data[0].ChannelName);
                $("#OutletCatagory").val(data[0].OutletCatagory);
                $("#signageheight").val(data[0].signageheight);
                $("#signagewidth").val(data[0].signagewidth);
                $("#remarks").val(data[0].remarks);
                $("#SE").val(data[0].SE);
                $("#PicUrl").val(data[0].PicUrl);
                $("#btnSave").prop('disabled', false);
                $("#sinage").val(data[0].sinage);
                $("#otherbrandd").val(data[0].otherbrandd);
                $("#sinagetype").val(data[0].sinagetype);
                $("#otherbrand").val(data[0].otherbrand);

                $("#signageheightNew").val(data[0].signageheightNew);
                $("#signagewidthNew").val(data[0].signagewidthNew);
                $("#feedback").val(data[0].feedback);
                $("#AppFlag1").val(data[0].AppFlag1);
                $("#AppFlag2").val(data[0].AppFlag2);
                $("#AppFlag3").val(data[0].AppFlag3);
                $("#OrderBy").val(data[0].OrderBy);
                $("#diff").val(data[0].diff);
                //window.location.reload();
            },
            error: function () {
                alert("Error!");
            }
            });

            $("#btnClick").prop('disabled', false);
        })

        function ItemCreated(id) {
            var answer = confirm('Are you sure you want to save this?');
            if (answer) {
                //  return;
                //return status;
            }
            else {
                return false;
            }
            var isAllValid = true;
            var status = true;
            if (isAllValid) {
                var data = {
                    id: $("#id").val(),
                    ReqID: $("#Req").val(),
                    VenID: $("#VenID").val(),
                    VenReqID: $("#VenReqID").val(),
                    RegionName: $("#RegionName").val(),
                    AreaName: $("#AreaName").val(),
                    CeAreaName: $("#CeAreaName").val(),
                    DbName: $("#DbName").val(),
                    cluster: $("#cluster").val(),
                    PSRName: $("#PSRName").val(),
                    SubRoute: $("#SubRoute").val(),
                    OutletCode: $("#OutletCode").val(),
                    OutLetName: $("#OutLetName").val(),
                    Address: $("#Address").val(),
                    OwnerName: $("#OwnerName").val(),
                    ContactNum: $("#ContactNum").val(),
                    ChannelName: $("#ChannelName").val(),
                    OutletCatagory: $("#OutletCatagory").val(),
                    sinage: $("#sinage").val(),
                    otherbrandd: $("#otherbrandd").val(),
                    sinagetype: $("#sinagetype").val(),
                    otherbrand: $("#otherbrand").val(),
                    signageheight: $("#signageheight").val(),
                    signagewidth: $("#signagewidth").val(),
                    signageheightNew: $("#signageheightNew").val(),
                    signagewidthNew: $("#signagewidthNew").val(),
                    remarks: $("#remarks").val(),
                    msg: $("#msg").val(),
                    dateLimit:$("#dateLimit").val(),
                    dateLimitfinal: $("#dateLimitfinal").val(),
                    feedback: $("#feedback").val(),
                    AppFlag3: $("#AppFlag3").val(),
                    OrderBy: $("#OrderBy").val()

                }
            }
            //console.log(data);
            debugger;

            $.ajax({
                url: '/OrderRequest/UpdateACKDataForVendor',
                type: "POST",
                data: JSON.stringify(data),
                dataType: "JSON",
                contentType: "application/json",
                success: function (a) {
                    //check is successfully save to database
                    if (a.status == true) {

                        alert('Successfully Done.');
                        window.location.reload();
                    }
                    else {
                        alert('Process is Failed!! Please Contact to IT dept.');
                    }

                },
                error: function () {
                    alert('Error. Please try again.');
                }
            });
        }



    function ReLoad(){
        alert("Are you sure reset the page ...?")
        location.reload();
    }

</script>

